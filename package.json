{
  "private": true,
  "scripts": {
    "build": "pnpm turbo run build --filter=@repo/types --filter=@repo/shared --filter=./apps/webApp --parallel",
    "native:dev": "turbo run dev --filter=@repo/types --filter=@repo/shared --filter=./apps/native --parallel",
    "web:dev": "turbo run dev --filter=@repo/types --filter=@repo/shared --filter=./apps/webApp --parallel",
    "shared": "turbo run dev --filter=@repo/shared --filter=@repo/types --parallel",
    "all": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "turbo run test"
  },
  "devDependencies": {
    "eslint": "^8",
    "prettier": "^3.2.5",
    "turbo": "^2.5.4",
    "typescript": "~5.8.3"
  },
  "packageManager": "pnpm@10.11.1",
  "name": "first-ride-repo",
  "version": "1.0.0",
  "pnpm": {
    "overrides": {
      "expo": "~53.0.20",
      "react": "19.0.0",
      "react-dom": "19.0.0",
      "react-native": "0.79.5",
      "metro": "^0.82.0",
      "metro-config": "^0.82.0",
      "metro-runtime": "^0.82.0",
      "metro-resolver": "^0.82.0",
      "metro-source-map": "^0.82.0",
      "zustand": "^5.0.3",
      "axios": "^1.9.0",
      "@tanstack/react-query": "^5.77.4",
      "@tanstack/react-query-devtools": "^5.77.4",
      "expo-secure-store": "^14.2.3"
    }
  }
}
